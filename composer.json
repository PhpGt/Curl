{
	"name": "phpgt/curl",
	"description": "cURL object wrapper.",
	"type": "library",

	"require": {
		"php": ">=7.2.0",
		"ext-curl": "*",
		"ext-json": "*"
	},

	"require-dev": {
		"phpstan/phpstan": "v1.8.0",
		"phpunit/phpunit": "v9.5.21"
	},

	"license": "MIT",

	"authors": [
	{
		"name": "Greg Bowler",
		"email": "greg.bowler@g105b.com"
	}
	],

	"autoload": {
		"psr-4": {
			"Gt\\Curl\\": "./src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\Curl\\Test\\": "./test/phpunit"
		}
	},

	"keywords": [
		"curl",
		"curl_multi",
		"interface",
		"http"
	],

	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/PhpGt"
		}
	]
}
